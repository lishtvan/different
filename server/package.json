    "@typescript-eslint/parser": "^6.7.2",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.9.2",
    "prettier": "^3.0.3",
