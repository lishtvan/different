{
  "name": "different",
  "version": "1.0.0",
  "description": "Brand clothes marketplace",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "18.9.0"
  },
  "scripts": {
    "build:ts": "tsc",
    "start": "npx prisma migrate deploy && tsc && fastify start dist/app.js --options --log-level=info --pretty-logs=true",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch='.ts$ .git node_modules database' -w -l info -P dist/app.js --options\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/oauth2": "^7.0.1",
    "@fastify/sensible": "5.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@fastify/websocket": "^7.2.0",
    "@prisma/client": "^4.13.0",
    "ajv-errors": "^3.0.0",
    "fastify": "^4.15.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "novaposhta": "^1.1.1",
    "twitter-api-sdk": "^1.2.1",
    "typesense": "^1.5.3"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.15.12",
    "@types/sharp": "^0.31.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.8.0",
    "prettier": "^2.8.7",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
