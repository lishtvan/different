    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "fastify-tsconfig": "^2.0.0",
    "json-schema-to-ts": "^3.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
