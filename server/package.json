    "@fastify/oauth2": "^7.4.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/sensible": "5.3.0",
    "@fastify/swagger": "^8.10.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/websocket": "^8.2.0",
    "@prisma/client": "^5.3.1",
    "ajv-errors": "^3.0.0",
