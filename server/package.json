{
  "name": "different",
  "version": "1.0.0",
  "description": "Brand clothes marketplace",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "18.6.0"
  },
  "scripts": {
    "build:ts": "tsc",
    "start": "fastify start -p -l info -P dist/app.js --options",
    "start:prod": "prisma generate && prisma migrate deploy && npm run start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch='.ts$ .git node_modules database' -w -l info -P dist/app.js --options\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.1.0",
    "@fastify/cookie": "^7.3.1",
    "@fastify/cors": "^8.0.0",
    "@fastify/multipart": "^7.1.0",
    "@fastify/oauth2": "^5.1.0",
    "@fastify/sensible": "5.1.0",
    "@fastify/swagger": "^7.4.1",
    "@prisma/client": "^4.1.0",
    "ajv-errors": "^3.0.0",
    "aws-sdk": "^2.1183.0",
    "fastify": "^4.3.0",
    "fastify-cli": "^4.3.0",
    "fastify-plugin": "^4.0.0",
    "hyperid": "^3.0.1",
    "sharp": "^0.30.7",
    "twitter-api-sdk": "^1.1.0",
    "weak-napi": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/sharp": "^0.30.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.5.5",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
